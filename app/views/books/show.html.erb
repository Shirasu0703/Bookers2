<div>
  <%= image_tag @book.get_image %>
  <p>Title<%= @book.title %></p>
  <p>Body<%= @book.body %></p>
  <p>投稿ユーザー画面：<%= image_tag @book.user.get_profile_image(100,100) %></p>
  <p><%= image_tag 'sample-author1.jpg' %></p>
  <p><%= @book.user.name %></p>
  <%if @book.user == current_user %>
    <%= link_to "destroy", books_path(@book), method: :delete %>
  <% end %>
</div>
