<div>
  <h3><%= @user.name %></h3>
  <%= image_tag @user.get_profile_image(100,100) %>
  <% if @user == current_user %>
  <p><%= link_to "プロフィール編集", edit_user_path(@user) %></p>
  <a data-confirm="本当にログアウトしますか？" rel="nofollow" data-method="delete" href="/users/sign_out">Log out</a>


</div>
<% end %>

<%= render 'books/book', books: @books %>